{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react",
    "strict": true,
    "noImplicitAny": false,
    "strictFunctionTypes": false,
    "esModuleInterop": true,
    "isolatedModules": true,
    "baseUrl": "src",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
